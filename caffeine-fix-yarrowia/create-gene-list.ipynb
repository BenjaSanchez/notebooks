   "source": [
    "import cobra\n",
    "import pandas as pd\n",
    "iYali_model = cobra.io.load_json_model(\"iYali-model.json\")\n",
    "df = pd.DataFrame()\n",
    "for gene in iYali_model.genes:\n",
    "    new_row = pd.DataFrame(index = [gene.id], data = {\"yali1\":[gene.name]})\n",
    "    df = df.append(new_row)\n",
    "df.to_csv(\"yali0to1.csv\")"
   ]
  }
